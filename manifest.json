{
  "manifest_version": 2,
  "name": "Tab Switcher",
  "author": "Kalle Persson",
  "version": "1.4.1",
  "homepage_url": "http://kallepersson.se/tabswitcher",
  "description": "Rapidly filter and switch between tabs",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "19": "icons/icon-19.png",
    "38": "icons/icon-38.png",
    "48": "icons/icon-48.png"
  },
  "browser_action": {
    "default_popup": "switcher.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K"
      }
    }
  }
}
